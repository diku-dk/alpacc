num = [0-9]+;
ignore = \s|\n|\t|\r;

Exp [Continue] -> Exp2 Exp_;
Exp_ [Add] -> "+" Exp2 Exp_;
Exp_ [Sub] -> "-" Exp2 Exp_;
Exp_ [Empty] -> ;

Exp2 [Continue] -> Exp3 Exp2_;
Exp2_ [Mul] -> "*" Exp3 Exp2_;
Exp2_ [Div] -> "/" Exp3 Exp2_;
Exp2_ [Empty] -> ;

Exp3 [Parentheses] -> "(" Exp ")";
Exp3 [Num] -> num;
